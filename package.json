{
  "name": "e2e-santa",
  "version": "1.0.0",
  "description": "",
  "main": "cypress.config.js",
  "scripts": {
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:run:allure": "npx cypress run --headed --reporter mocha-allure-reporter",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -r allure-report/* & rm -r allure-results/*",
    "allure:open": "allure serve allure-results"
  },
  "author": "Daria Zakharova",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.1.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.1",
    "@faker-js/faker": "^7.6.0",
    "cypress": "^13.13.0",
    "esbuild": "^0.23.0",
    "mocha-allure-reporter": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dariadao/e2e-santa.git"
  },
  "keywords": [
    "cypress"
  ],
  "bugs": {
    "url": "https://github.com/Dariadao/e2e-santa/issues"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/support/step_definitions/santa-box-create.cy.js"
  },
  "devDependencies": {
    "allure-commandline": "^2.29.0",
    "allure-cypress": "^3.0.0-beta.7"
  }
}
